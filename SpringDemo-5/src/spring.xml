<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
 
 <beans>
 	<bean id="triangle" class="org.collectionInitalizationInSpring.example.Triangle" >
 	
 	<!-- Usage of list tag for collections with setter injection type -->
 		<property name="points">
 			<list>
 				<ref bean="zeroPoint"></ref>
 				<ref bean="point2"></ref>
 				<ref bean="point3"></ref>
 			</list>
 		</property>
 	</bean>
 	
 	
 	<bean id="zeroPoint" class="org.collectionInitalizationInSpring.example.Point">
 		<property name="x" value="0" />
 		<property name="y" value="0" />
 	</bean>
 	
 	
 	<bean id="point2" class="org.collectionInitalizationInSpring.example.Point">
 		<property name="x" value="-20" />
 		<property name="y" value="0" />
 	</bean>
 	
 	
 	<bean id="point3" class="org.collectionInitalizationInSpring.example.Point">
 		<property name="x" value="0" />
 		<property name="y" value="20" />
 	</bean>
 	
 </beans>
