<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
 
 <beans>
 	<bean id="triangle" class="org.innerobjectsAliasesIdRef.usage.example.Triangle" >
 	
 		<property name="pointA" ref="zeroPoint"></property>
 		
 		<!-- Usage of Inner Beans -->
 		<property name="pointB"> 
 			<bean class="org.innerobjectsAliasesIdRef.usage.example.Point">
		 		<property name="x" value="-20" />
		 		<property name="y" value="0" />
 			</bean>
 		</property>
 		
 		<property name="pointC">
	 		<bean id="point3" class="org.innerobjectsAliasesIdRef.usage.example.Point">
		 		<property name="x" value="0" />
		 		<property name="y" value="20" />
 			</bean>
 		</property>
 	</bean>
 	
 	<bean id="zeroPoint" class="org.innerobjectsAliasesIdRef.usage.example.Point">
 		<property name="x" value="0" />
 		<property name="y" value="0" />
 	</bean>
 	
 	<!-- usage of aliases -->
 	<alias name="triangle" alias="triangle-alias"></alias>
 </beans>
